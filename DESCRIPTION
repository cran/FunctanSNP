Package: FunctanSNP
Type: Package
Title: Functional Analysis (with Interactions) for Dense SNP Data
Version: 0.1.0
Authors@R: c(person("Rui", "Ren", role=c("aut", "cre"),
             email = "xmurr@stu.xmu.edu.cn"),
             person("Kuangnan", "Fang", role=c("aut")),
